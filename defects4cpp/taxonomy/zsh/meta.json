  "common": {
    "exclude": [],
    "build": {
      "command": [
        "sh -c \"autoheader && autoconf\"",
        "sh -c \"./configure --with-tcsetpgrp\"",
        "sh -c \"make -j@DPP_PARALLEL_BUILD@\"",
        "sh -c \"sed -i 's|sleep 1;||' Test/Makefile\""
      ]
    },
    "build-coverage": {
      "command": [
        "sh -c \"autoheader && autoconf\"",
        "sh -c \"./configure --with-tcsetpgrp CFLAGS=\"--coverage -g -O0\" LDFLAGS=-lgcov\"",
        "sh -c \"make -j@DPP_PARALLEL_BUILD@\"",
        "sh -c \"sed -i 's|sleep 1;||' Test/Makefile\""
      ]
    },
    "test-type": "automake",
    "test": {
      "command": [
        "bash -c 'ztsts=(Test/*.ztst); make -s TESTNUM=$(basename ${ztsts[$(cat DPP_TEST_INDEX)-1]} .ztst) check'"
      ]
    },
    "test-coverage": {
      "command": [
        "sh -c 'make check'"
      ]
    },
    "gcov": {
      "exclude": [],
      "command": [
        "sh -c \"mkdir gcov && find . -type f -name '*.gcov' -exec cp --parent {} /home/workspace/gcov \\;\"",
        "sh -c \"cd gcov && gcovr -g --html > coverage.html\""
      ]
    }
  },
